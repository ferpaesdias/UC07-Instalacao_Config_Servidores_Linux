$TTL    604800
@       IN      SOA     dns.empresatech.example. admin.empresatech.example. (
                        2025100501 ; Serial
                        604800     ; Refresh
                        86400      ; Retry
                        2419200    ; Expire
                        604800 )   ; Negative Cache TTL
;
@       IN      NS      dns.empresatech.example.

; Servidores da DMZ
dns     IN      A       10.0.2.2
web     IN      A       10.0.2.3

; Servidores da rede interna
dhcp    IN      A       10.0.3.100
filesrv IN      A       10.0.3.101
ldap    IN      A       10.0.3.102
pc01    IN      A       10.0.3.10

; Alias
www     IN      CNAME   web.empresatech.example.
